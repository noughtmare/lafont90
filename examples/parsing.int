type stream, tree

symbol + : stream+; stream-
       1 : stream+; stream-
       Plus : tree+; tree-, tree-
       One : tree+
       Parse : stream-; {tree+, stream+}
       End : stream+

+[Parse(v, Parse(w,y))] >< Parse[Plus(v, w), y]
                   1[x] >< Parse[One, x]
                   
                   
init Parse(Print, Print) -><- +(1(1(End)))